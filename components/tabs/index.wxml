<scroll-view wx:if="{{ scroll }}" bindscroll="onScroll" scroll-x scroll-left="{{scrollLeft}}" scroll-with-animation class="i-class i-tabs i-tabs-scroll {{ fixed ? 'i-tabs-fixed' : '' }}">
  <slot></slot>
</scroll-view>
<view wx:else class="i-class i-tabs {{ fixed ? 'i-tabs-fixed' : '' }}">
  <slot></slot>
</view>